DROP MATERIALIZED VIEW dynamic_groups_by_partner;

ALTER TABLE dynamic_groups DROP updated_by;

CREATE MATERIALIZED VIEW dynamic_groups_by_partner AS
SELECT id, name, group_definition, query, created_at, updated_at, created_by, client, partner, site
FROM dynamic_groups
WHERE id IS NOT NULL AND partner IS NOT NULL
PRIMARY KEY ((partner), id);



