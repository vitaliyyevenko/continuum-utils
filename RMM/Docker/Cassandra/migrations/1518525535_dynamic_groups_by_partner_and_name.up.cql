CREATE MATERIALIZED VIEW IF NOT EXISTS dynamic_groups_by_partner_and_name AS
SELECT id, name, group_definition, query, created_at, updated_at, updated_by, created_by, client, partner, site
FROM dynamic_groups
WHERE id IS NOT NULL AND partner IS NOT NULL AND name IS NOT NULL
PRIMARY KEY ((partner, name), id);