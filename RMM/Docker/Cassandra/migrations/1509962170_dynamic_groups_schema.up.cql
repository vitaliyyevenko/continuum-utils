ALTER TABLE dynamic_groups ADD updated_by text;

DROP MATERIALIZED VIEW IF EXISTS dynamic_groups_by_partner;

CREATE MATERIALIZED VIEW IF NOT EXISTS dynamic_groups_by_partner AS
SELECT id, name, group_definition, query, created_at, updated_at, updated_by, created_by, client, partner, site
FROM dynamic_groups
WHERE id IS NOT NULL AND partner IS NOT NULL
